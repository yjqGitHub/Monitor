@{
    Layout = null;
}
<!DOCTYPE HTML>
<html>
<head>
    <meta charset="utf-8">
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" />
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <!--[if lt IE 9]>
    <script type="text/javascript" src="/Content/lib/html5shiv.js"></script>
    <script type="text/javascript" src="/Content/lib/respond.min.js"></script>
    <![endif]-->
    <link href="/Content/static/h-ui/css/H-ui.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/static/h-ui.admin/css/H-ui.login.css" rel="stylesheet" type="text/css" />
    <link href="/Content/static/h-ui.admin/css/style.css" rel="stylesheet" type="text/css" />
    <link href="/Content/lib/Hui-iconfont/1.0.8/iconfont.css" rel="stylesheet" type="text/css" />
    <!--[if IE 6]>
    <script type="text/javascript" src="/Content/lib/DD_belatedPNG_0.0.8a-min.js" ></script>
    <script>DD_belatedPNG.fix('*');</script>
    <![endif]-->
    <title>后台登录 - 系统监控</title>
    <meta name="keywords" content="系统监控">
    <meta name="description" content="系统监控">
</head>
<body>
    <div class="header"></div>
    <div class="loginWraper">
        <div id="loginform" class="loginBox">
            <form class="form form-horizontal">
                <div class="row cl">
                    <label class="form-label col-xs-3"><i class="Hui-iconfont">&#xe60d;</i></label>
                    <div class="formControls col-xs-8">
                        <input id="" name="" type="text" placeholder="账户" class="input-text size-L" data-check="1" data-expression="NotNull" data-err="账户">
                    </div>
                </div>
                <div class="row cl">
                    <label class="form-label col-xs-3"><i class="Hui-iconfont">&#xe60e;</i></label>
                    <div class="formControls col-xs-8">
                        <input id="" name="" type="password" placeholder="密码" class="input-text size-L" data-check="1" data-expression="NotNull" data-err="密码">
                    </div>
                </div>
                <div class="row cl">
                    <div class="formControls col-xs-8 col-xs-offset-3">
                        <input class="input-text size-L" type="text" placeholder="验证码" id="FCode" name="FCode" style="width:150px;">
                        <img src="/Account/ValidateCode" alt="点击切换验证码" title="点击切换验证码" style="margin-top: 0px; vertical-align: top; cursor: pointer;" onclick="ToggleCode(this, '/Account/ValidateCode'); return false;">
                    </div>
                </div>
                <div class="row cl">
                    <div class="formControls col-xs-8 col-xs-offset-3">
                        <input name="" type="submit" class="btn btn-success radius size-L" value="&nbsp;登&nbsp;&nbsp;&nbsp;&nbsp;录&nbsp;">
                        <input name="" type="reset" class="btn btn-default radius size-L" value="&nbsp;取&nbsp;&nbsp;&nbsp;&nbsp;消&nbsp;">
                    </div>
                </div>
            </form>
        </div>
    </div>
    <div id="loading" onclick="LoadingFunc(false);">
        正在处理，请稍等。。。
    </div>
    <div class="footer">Copyright Open Source by yjq</div>
    <script type="text/javascript" src="/Content/lib/jquery/1.9.1/jquery.min.js"></script>
    <script type="text/javascript" src="/Content/static/h-ui/js/H-ui.min.js"></script>
    <script src="/Content/JS/DomainJs/Account.js"></script>
    <script src="/Content/JS/CommonJs/DialogJs.js"></script>
    <script src="~/Content/JS/CommonJs/Validator.js"></script>
</body>
</html>
